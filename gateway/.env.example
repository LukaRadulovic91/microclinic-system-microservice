APP_NAME=UserService
APP_ENV=local
APP_KEY=base64:GENERATE_YOUR_KEY_HERE
APP_DEBUG=true
APP_URL=http://localhost:8001

LOG_CHANNEL=stack
LOG_LEVEL=debug

DB_CONNECTION=pgsql
DB_HOST=postgres
DB_PORT=5432
DB_DATABASE=microclinic
DB_USERNAME=microclinic_user
DB_PASSWORD=secretpassword

BROADCAST_DRIVER=rabbitmq
CACHE_DRIVER=file
QUEUE_CONNECTION=rabbitmq
SESSION_DRIVER=file
SESSION_LIFETIME=120

RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/

# Passport settings
PASSPORT_CLIENT_ID=2
PASSPORT_CLIENT_SECRET=YOUR_PASSPORT_CLIENT_SECRET

# API version prefix
API_PREFIX=/api/v1
